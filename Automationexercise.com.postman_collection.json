        {
          "name": "Pagination",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// 1. Configuration - Set your limit here\r",
                  "const MAX_PAGES_TO_TEST = Number(pm.collectionVariables.get(\"MAX_PAGES_TO_TEST\"));\r",
                  "console.log(\"Maximum pages to test is COnfigured to : \" + MAX_PAGES_TO_TEST);\r",
                  "\r",
                  "// 2. Parse response\r",
                  "const jsonData = pm.response.json();\r",
                  "let nexturl = jsonData.info.next;\r",
                  "\r",
                  "// 3. Logic to extract page number and decide to loop\r",
                  "if (nexturl !== null) {\r",
                  "    let parsedUrl = new URL(nexturl);\r",
                  "    let pageNumber = parseInt(parsedUrl.searchParams.get(\"page\"));\r",
                  "    \r",
                  "    console.log(\"Next Page detected: \" + pageNumber);\r",
                  "\r",
                  "    // 4. CHECK: Is the next page within our limit?\r",
                  "    if (pageNumber <= MAX_PAGES_TO_TEST) {\r",
                  "        pm.collectionVariables.set(\"currentpage\", pageNumber);\r",
                  "        \r",
                  "        console.log(`Limit not reached. Proceeding to page ${pageNumber}...`);\r",
                  "        pm.execution.setNextRequest(\"Pagination\");\r",
                  "    } else {\r",
                  "        console.log(`Reached Max Page limit (${MAX_PAGES_TO_TEST}). Stopping loop.`);\r",
                  "        pm.execution.setNextRequest(null);\r",
                  "        pm.collectionVariables.set(\"currentpage\", 1);\r",
                  "    }\r",
                  "\r",
                  "} else {\r",
                  "    console.log(\"Reached the end of the API data. Stopping.\");\r",
                  "    pm.execution.setNextRequest(null);\r",
                  "}"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            },
